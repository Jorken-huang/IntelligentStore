            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="金融" FontSize="25"/>
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="新闻天气" FontSize="25"/>
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="音乐视频" FontSize="25"/>
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="健康饮食" FontSize="25"/>
